{
  "version": 2,
  "buildCommand": "",
  "devCommand": "npm run dev",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/test", "dest": "/api/test.js" },
    { "src": "/api/test-db", "dest": "/api/test-db.js" },
    { "src": "/api/save-score", "methods": ["POST"], "dest": "/api/save-score.js" },
    { "src": "/api/top-users", "dest": "/api/top-users.js" }
  ]
}